<!doctype html>
<html lang="en">

<head>
{{> head }}
</head>

<body>
    {{> header }}
    <div class="main_div" style="height: 600px">
        <h1>Login</h1>
        <form method="post" id="form" action="/auth/login" >
            <div style="padding: 10px"><input type="text" name="login" placeholder="Enter Login" minlength="3" maxlength="12"></div>
            <div style="padding: 10px"><input type="password" name="password" placeholder="Enter password" required></div>
            <div style="padding: 10px"><input type="submit"  value="Lohin" ></div>
        </form>
        <form method="post" id="form" action="/auth/auth/google" >
            <div style="padding: 10px"><input type="submit"  value="Log in with Google" ></div>
        <form>
    </div>
</body>

</html>